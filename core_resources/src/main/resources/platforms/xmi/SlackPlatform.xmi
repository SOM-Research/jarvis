<?xml version="1.0" encoding="ASCII"?>
<platform:PlatformDefinition xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:platform="fr.zelus.jarvis.platform" name="SlackPlatform" runtimePath="fr.zelus.jarvis.plugins.slack.platform.SlackPlatform">
  <eventProviderDefinitions xsi:type="platform:InputProviderDefinition" name="SlackIntentProvider"/>
  <actions name="PostMessage">
    <parameters key="message"/>
    <parameters key="channel"/>
  </actions>
  <actions name="Reply">
    <parameters key="message"/>
  </actions>
  <actions name="PostFileMessage">
